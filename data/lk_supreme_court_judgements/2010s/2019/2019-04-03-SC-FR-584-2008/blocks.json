    "fonts": [
      "Times New Roman"
    ],
    "sizes": [
      9.0,
      14.039999961853027
    ]
  },
  {
    "page_number": 4,
    "bbox": [
      72.02,
      515.68,
      543.34,
      531.22
    ],
