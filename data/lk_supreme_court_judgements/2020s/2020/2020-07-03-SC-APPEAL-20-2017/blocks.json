  },
  {
    "page_number": 5,
    "bbox": [
      72.02,
      38.16,
      482.84,
      49.2
    ],
    "text": "SC Appeal 20/2017",
    "fonts": [
      "Calibri"
    ],
    "sizes": [
      11.039999961853027
    ]
  },
